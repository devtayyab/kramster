{
  "name": "kramster",
  "version": "1.0.0",
  "description": "Kramster is a quiz app for making exam preparation more fun",
  "main": "src/server/server.js",
  "scripts": {
    "start": "nodemon src/server/server.js --exec babel-node",
    "dev": "webpack-dev-server --inline --content-base src/client",
    "build": "export NODE_ENV=production && webpack -p --progress && babel server.js -o server.dist.js",
    "prod": "npm run build && node server.dist.js",
    "lint": "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/draperunner/Kramster.git"
  },
  "author": "Mats Byrkjeland",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.12.3",
    "chart.js": "^2.5.0",
    "express": "^4.13.3",
    "mongoose": "^4.8.2",
    "qs": "^6.3.0",
    "react": "^15.4.1",
    "react-chartjs-2": "^2.0.5",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "sanitize-html": "^1.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "ink-docstrap": "^1.1.4",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "should": "^8.3.0",
    "style-loader": "^0.13.1",
    "supertest": "^1.2.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0"
  }
}
