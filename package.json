{
  "name": "kramster",
  "version": "2.0.0",
  "description": "Kramster is a quiz app for making exam preparation more fun",
  "main": "src/server/server.js",
  "scripts": {
    "start": "nodemon src/server/server.js --exec babel-node",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js && babel src/server -d dist/server",
    "prod": "npm run build && NODE_ENV=production node dist/server/server.js",
    "lint": "eslint --ext js,jsx src/ && stylelint src/**/*.css",
    "test": "mocha",
    "deploy": "./deploy-production.sh",
    "deploy:staging": "./deploy-staging.sh"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/draperunner/Kramster.git"
  },
  "author": "Mats Byrkjeland",
  "license": "Apache-2.0",
  "dependencies": {
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "chart.js": "^2.7.3",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "mongoose": "^4.13.17",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.5.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.5.2",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "^2.5.3",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "redux": "^4.0.1",
    "sanitize-html": "^1.19.1",
    "wilson-score": "^1.0.0"
