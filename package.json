{
  "name": "kramster",
  "version": "2.0.0",
  "description": "Kramster is a quiz app for making exam preparation more fun",
  "main": "src/server/server.js",
  "scripts": {
    "start": "DOTENV_CONFIG_PATH=.env.dev webpack-dev-server --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "build:staging": "DOTENV_CONFIG_PATH=.env.staging npm run build",
    "build:prod": "DOTENV_CONFIG_PATH=.env npm run build",
    "prod": "npm run build:prod && NODE_ENV=production node dist/server/server.js",
    "eslint": "eslint --ext js,jsx,ts,tsx src/",
    "stylelint": "stylelint src/**/*.css",
    "lint": "npm run eslint && npm run stylelint",
    "test": "mocha",
    "deploy": "npm run build:prod && firebase deploy -P prod",
    "deploy:staging": "npm run build:staging && firebase deploy -P staging"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/draperunner/Kramster.git"
  },
  "author": "Mats Byrkjeland",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/qs": "^6.9.1",
    "chart.js": "^2.9.3",
    "katex": "^0.11.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "^2.7.0",
    "react-router": "^3.2.6",
    "sanitize-html": "^1.22.1",
    "wilson-score": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/katex": "^0.11.0",
    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.6",
    "@types/react-router": "^3.0.22",
    "@types/sanitize-html": "^1.22.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "prettier": "^2.0.4",
    "react-hot-loader": "^4.12.20",
    "react-transform-hmr": "^1.0.4",
    "should": "^13.2.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^13.3.0",
    "stylelint-config-standard": "^20.0.0",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
