    "should": "^13.2.3",
    "style-loader": "^0.21.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "supertest": "^3.3.0",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
