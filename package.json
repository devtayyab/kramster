{
  "name": "kramster",
  "version": "2.0.0",
  "description": "Kramster is a quiz app for making exam preparation more fun",
  "main": "src/server/server.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "build:staging": "DOTENV_CONFIG_PATH=.env.staging npm run build",
    "prod": "npm run build && NODE_ENV=production node dist/server/server.js",
    "eslint": "eslint --ext js,jsx src/",
    "stylelint": "stylelint src/**/*.css",
    "lint": "npm run eslint && npm run stylelint",
    "test": "mocha",
    "deploy": "npm run build && firebase deploy -P prod",
    "deploy:staging": "npm run build:staging && firebase deploy -P staging"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/draperunner/Kramster.git"
  },
  "author": "Mats Byrkjeland",
  "license": "Apache-2.0",
  "dependencies": {
    "chart.js": "^2.8.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "^2.5.7",
    "react-redux": "^5.1.1",
    "react-router": "^3.2.1",
    "redux": "^4.0.1",
    "sanitize-html": "^1.20.1",
    "whatwg-fetch": "^3.0.0",
    "wilson-score": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "ink-docstrap": "^1.3.2",
    "json-loader": "^0.5.4",
    "mocha": "^5.2.0",
    "react-hot-loader": "^4.8.4",
    "react-transform-hmr": "^1.0.4",
    "should": "^13.2.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "supertest": "^3.4.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-merge": "^4.2.1"
  }
}
