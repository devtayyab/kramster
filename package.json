{
  "name": "kramster",
  "version": "2.0.0",
  "description": "Kramster is a quiz app for making exam preparation more fun",
  "main": "src/server/server.js",
  "scripts": {
    "start": "DOTENV_CONFIG_PATH=.env.dev webpack-dev-server --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "build:staging": "DOTENV_CONFIG_PATH=.env.staging npm run build",
    "build:prod": "DOTENV_CONFIG_PATH=.env npm run build",
    "prod": "npm run build:prod && NODE_ENV=production node dist/server/server.js",
    "eslint": "eslint --ext js,jsx,ts,tsx src/",
    "stylelint": "stylelint src/**/*.css",
    "lint": "npm run eslint && npm run stylelint",
    "test": "mocha",
    "deploy": "npm run build:prod && firebase deploy -P prod",
    "deploy:staging": "npm run build:staging && firebase deploy -P staging"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/draperunner/Kramster.git"
  },
  "author": "Mats Byrkjeland",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/qs": "^6.9.0",
    "chart.js": "^2.9.3",
    "katex": "^0.10.2",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.12.0",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "^2.7.0",
    "react-redux": "^5.1.2",
    "react-router": "^3.2.5",
    "redux": "^4.0.4",
    "sanitize-html": "^1.20.1",
    "whatwg-fetch": "^3.0.0",
    "wilson-score": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/katex": "^0.11.0",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^3.0.20",
    "@types/sanitize-html": "^1.20.2",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "ink-docstrap": "^1.3.2",
    "json-loader": "^0.5.4",
    "mocha": "^5.2.0",
    "react-hot-loader": "^4.12.18",
    "react-transform-hmr": "^1.0.4",
    "should": "^13.2.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
